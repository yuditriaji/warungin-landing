  <!-- Pricing Toggle Script -->
  <script>
    document.addEventListener('DOMContentLoaded', () => {
      const prices = {
        pemula: { monthly: 49000, quarterly: 132000, yearly: 470000 },
        bisnis: { monthly: 149000, quarterly: 399000, yearly: 1430000 },
      };

      const periodLabels = {
        monthly: '/ bulan',
        quarterly: '/ 3 bulan',
        yearly: '/ tahun',
      };

      function formatPrice(amount) {
        if (amount >= 1000000) {
          const val = amount / 1000000;
          return 'Rp ' + (Number.isInteger(val) ? val : val.toFixed(1)) + 'JT';
        }
        return 'Rp ' + Math.round(amount / 1000) + 'K';
      }

      function updatePrices(period) {
        document.querySelectorAll('.price-display').forEach(el => {
          const plan = el.dataset.plan;
          if (!prices[plan]) return;

          const price = prices[plan][period];
          const monthlyPrice = prices[plan].monthly;
          const mainEl = el.querySelector('[data-price-main]');
          const periodEl = el.querySelector('[data-price-period]');
          const savingsEl = el.querySelector('[data-price-savings]');

          // Animate price change
          mainEl.style.opacity = '0';
          mainEl.style.transform = 'translateY(-8px)';

          setTimeout(() => {
            mainEl.textContent = formatPrice(price);
            periodEl.textContent = periodLabels[period];

            if (period !== 'monthly') {
              const months = period === 'quarterly' ? 3 : 12;
              const perMonth = Math.round(price / months);
              const savings = Math.round(((monthlyPrice - perMonth) / monthlyPrice) * 100);
              savingsEl.textContent = `â‰ˆ Rp ${Math.round(perMonth / 1000)}K/bulan (hemat ${savings}%)`;
              savingsEl.classList.remove('hidden');
            } else {
              savingsEl.classList.add('hidden');
            }

            mainEl.style.opacity = '1';
            mainEl.style.transform = 'translateY(0)';
          }, 150);
        });
      }

      // Toggle buttons
      document.querySelectorAll('.billing-btn').forEach(btn => {
        btn.addEventListener('click', () => {
          // Update active state
          document.querySelectorAll('.billing-btn').forEach(b => {
            b.classList.remove('bg-white', 'text-purple-700', 'shadow-sm');
            b.classList.add('text-slate-600');
          });
          btn.classList.add('bg-white', 'text-purple-700', 'shadow-sm');
          btn.classList.remove('text-slate-600');

          updatePrices(btn.dataset.period);
        });
      });
    });
  </script>

